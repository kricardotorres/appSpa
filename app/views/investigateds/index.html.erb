<p id="notice"><%= notice %></p>

<h1>Investigateds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lastname1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investigateds.each do |investigated| %>
      <tr>
        <td><%= investigated.name %></td>
        <td><%= investigated.lastname1 %></td>
        <td><%= link_to 'Show', investigated %></td>
        <td><%= link_to 'Edit', edit_investigated_path(investigated) %></td>
        <td><%= link_to 'Destroy', investigated, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investigated', new_investigated_path %>
